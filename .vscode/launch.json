{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run main_web_test.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/app/main_web_test.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Run main_dev.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/app/main_dev.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        
        {
            "name": "Run main.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/app/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Run Uvicorn (FastAPI)",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "src.app.main:app",
                "--reload"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}